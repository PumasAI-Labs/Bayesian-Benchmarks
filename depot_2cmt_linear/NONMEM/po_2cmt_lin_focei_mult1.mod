$PROB 2 cmt po lin
$DATA ../../data/sd_po_2cmt_lin.csv  IGNORE=@
$INPUT ID TIME CP DV AMT EVID CMT RATE DOSEGRP
$SUB ADVAN6 TOL=8
$MODEL COMP=(DEPOT) COMP=(CENTRAL, DEFOBS) COMP = (PERIPHERAL)

$PK
TVCL = THETA(1)
TVV1 = THETA(2)
TVQ = THETA(3)
TVV2 = THETA(4)
TVKA = THETA(5)
CL = TVCL * EXP(ETA(1))
V1 = TVV1 * EXP(ETA(2))
Q = TVQ * EXP(ETA(3))
V2 = TVV2 * EXP(ETA(4))
KA = TVKA * EXP(ETA(5))
S2 = V1/1000

$DES
DADT(1) = -KA*A(1)
DADT(2) = KA*A(1)-(CL/V1)*A(2) - (Q/V1)*A(2) + (Q/V2)*A(3)
DADT(3) = (Q/V1)*A(2) - (Q/V2)*A(3)

$ERROR
IPRED=F
Y = IPRED*(1 + ERR(1))
$THETA
(0,4)
(0,70)
(0,4)
(0,50)
(0,1)

$OMEGA
0.09
0.09
0.09
0.09
0.09

$SIGMA
0.04

$EST METHOD=1 INTER MAXEVAL=9999

$COV PRINT=E
